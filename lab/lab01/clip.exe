def double_eights(n):
    """Return true if n has two eights in a row.
    >>> double_eights(8)
    False
    >>> double_eights(88)
    True
    >>> double_eights(2882)
    True
    >>> double_eights(880088)
    True
    >>> double_eights(12345)
    False
    >>> double_eights(80808080)
    False
    """
    "*** YOUR CODE HERE ***"
    if n // 10 == 0:
        return False
    last, n = n % 10, n // 10
    print("DEBUG: last value: ", last)
    print("DEBUG: ", last == 8)
    assert last == 8, "Fomer part of condition evaluated to false"
    if last == 8 and n % 10 == 8:
        return True
    else:
        double_eights(n)
